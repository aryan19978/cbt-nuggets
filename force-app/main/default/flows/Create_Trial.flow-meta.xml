    <formulas>
        <name>AdminEmail</name>
        <dataType>String</dataType>
        <expression>IF({!GetEntity.DeveloperName} = &apos;Lead&apos;, {!Get_Lead_Record.Email}, IF({!GetEntity.DeveloperName} = &apos;Opportunity&apos;, {!Get_Primary_Contact.Email}, &apos;&apos;))</expression>
