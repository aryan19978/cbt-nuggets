    <formulas>
        <name>DurationOverrideValidation</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND({!Duration_Override} &gt; 0,{!Duration_Override} &lt; 366 ), true, false)</expression>
    </formulas>
    <formulas>
        <name>IdPrefix</name>
        <dataType>String</dataType>
        <expression>Left({!recordId},3)</expression>
    </formulas>
    <formulas>
        <name>LicenceCount</name>
        <dataType>Number</dataType>
        <expression>IF({!GetEntity.DeveloperName} = &apos;Lead&apos;,         {!Get_Lead_Record.License_Count__c},       IF({!GetEntity.DeveloperName} = &apos;Opportunity&apos;, {!Get_Opportunity_Record.License_Count__c}, 1))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>LicenceCountValidation</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND({!License_Count} &gt;0 , {!License_Count} &lt;101 ), true, false)</expression>
    </formulas>
    <formulas>
        <name>LicenseType</name>
        <dataType>String</dataType>
        <expression>IF({!GetEntity.DeveloperName} = &apos;Lead&apos;,   TEXT({!Get_Lead_Record.License_Type__c}) ,   IF({!GetEntity.DeveloperName} = &apos;Opportunity&apos;, TEXT({!Get_Opportunity_Record.License_Type__c}), &apos;Annual&apos;) )</expression>
    </formulas>
    <formulas>
